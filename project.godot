config_version=5

[application]

config/name="Eldritch Echoes"
run/main_scene="res://levels/level_1.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0.117647, 0.0745098, 1)
boot_splash/image="res://ui/TempLogo1.png"
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio/default_bus_layout.tres"

[autoload]

Global="*res://global.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"

[display]

window/size/viewport_width=300
window/size/viewport_height=120
window/size/window_width_override=1280
window/size/window_height_override=720
window/vsync/vsync_mode=0
mouse_cursor/custom_image="res://weapons/crosshairs/crosshair176.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://audio/": "red",
"res://characters/": "teal",
"res://dialogue/": "blue",
"res://effects/": "orange",
"res://environment/": "green",
"res://levels/": "yellow",
"res://ui/": "pink",
"res://weapons/": "purple"
}

